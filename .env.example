# ERPNext API Configuration
# Copy this file to .env and update the values

# Your ERPNext site URL (without trailing slash)
VITE_ERPNEXT_API_URL=https://your-erpnext-site.com

# API Key and Secret for authentication
# Generate from: Settings > API Access > Generate Keys
VITE_ERPNEXT_API_KEY=your_api_key_here
VITE_ERPNEXT_API_SECRET=your_api_secret_here
